#option-pricing.col-sm-4
  #results_div
  = form_tag( :option_pricing_calcuate_option_pricing ,method: :post, remote: :true ) do |f|
    %table
      #stock_price_block
        %tr
          %td
            = label_tag :Stock_Price
          %td
            = number_field_tag :stock_price, :class => 'text_field', :step => "0.01",:min => "0"
          %td
            = number_field_tag :stock_price_max, min: 0, step: 0.5, disabled: true 
      %tr 
        %td
          = label_tag :Strike_price
        %td
          = number_field_tag :strike_price, :class => 'text_field', :step => "0.01",:min => "0"
      %tr 
        %td
          = label_tag :Duration
        #duration_range
          %td
            = number_field_tag :duration, :class => 'text_field', :step => "0.01",:min => "0"
      %tr
        %td
          = label_tag :Interest_rate
        #interest_rate_range
          %td
            = number_field_tag :interest_rate, :class => 'text_field', :step => "0.01",:min => "0"
      %tr
        %td
          = label_tag :volatality
        #volatality_range
          %td
            = number_field_tag :volatality_min,nil, placeholder: 'Min Value', :class => 'text_field', :step => 0.5
          %td
            = number_field_tag :volatality_max,nil, placeholder: 'Max Value', :class => 'text_field', :step => 0.5  
      %tr
        .radio
          %td
            = radio_button_tag 'call_put_flag', 'call'
            %p Call 
          %td
            = radio_button_tag 'call_put_flag', 'put'
            %p Put
      %tr
        %td
          =submit_tag :Submit, class: 'btn btn-info'

:javascript

  $(document).ready(function(){
    $('tr td').eq(2).hide();
  });
  $('.stock_price_tick').click(function(){
  debugger
    $('#stock_price_block tr td').eq(2).show();
  });